#include <iostream>
#include <cmath>
using namespace std;

double f(double x)
{
    return 5.6 / (pow(x, 3) + 1) + 0.3 * x - pow(sin(x), 3);
}

int main()
{
    setlocale(LC_ALL, "Russian");

    double x_n = -10;
    double x_k = 10;
    double dx = 0.5;

    cout << "x\tF(x)\n";

    while (x_n <= x_k)
    {
        double result = f(x_n);

        if (to_string(result) == "inf")
        {
            cout << x_n << "\tФункция не определена\n";
        }
        else
        {
            cout << x_n << "\t" << result << '\n';
        }

        x_n += dx;
    }
}
