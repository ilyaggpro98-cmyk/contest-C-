#include <bits/stdc++.h>
using namespace std;
bool hasOddLetterCount(const string &line)
{
    int count = 0;
    for (char ch : line)
    {
        if (ch >= 'A' && ch <= 'Z') count++;
    }
    return count % 2 == 1;
}
int main()
{
    setlocale(LC_ALL,"Russian");

    ifstream fin("F1.txt");
    ofstream fout("Result.txt");
    if (!fin.is_open() || !fout.is_open())
    {
        cerr << "Ошибка открытия файла!" << endl;
        return 1;
    }
    string line;
    while (getline(fin, line))
    {
        if (hasOddLetterCount(line))
        {
            fout << line << "\n";
        }
    }
    fin.close();
    fout.close();

}
