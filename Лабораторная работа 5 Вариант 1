#include <iostream>
#include <string>
using namespace std;

int main()
{
    setlocale(LC_ALL, "Russian");
    int n;
    cin >> n;

    switch (n)
    {
        case 1:
            cout << "Предыдущий день недели: Воскресенье\n";
            break;
        case 2:
            cout << "Предыдущий день недели: Понедельник\n";
            break;
        case 3:
            cout << "Предыдущий день недели: Вторник\n";
            break;
        case 4:
            cout << "Предыдущий день недели: Среда\n";
            break;
        case 5:
            cout << "Предыдущий день недели: Четверг\n";
            break;
        case 6:
            cout << "Предыдущий день недели: Пятница\n";
            break;
        case 7:
            cout << "Предыдущий день недели: Суббота\n";
            break;
        default:
            cout << "Неправильный номер дня недели\n";
            return 0;
    }

    return 0;
}
